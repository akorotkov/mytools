net:
  bindIp: 0.0.0.0
  port: 27017
processManagement:
  fork: true
replication:
  oplogSizeMB: 153600
  replSetName: foo
setParameter:
  enableTestCommands: 1
storage:
  dbPath: /data/m/mo
  engine: wiredTiger
systemLog:
  destination: file
  path: /data/m/mo/mongod.log

storage.wiredTiger.collectionConfig.blockCompressor: snappy
storage.wiredTiger.engineConfig.journalCompressor: snappy
storage.wiredTiger.engineConfig.cacheSizeGB: 2
